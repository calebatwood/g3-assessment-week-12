<table>
  <thead>
    <tr>
      <th>Movie</th>
      <th>Reviewer</th>
      <th>Date</th>
    </tr>
  </thead>
  <tbody>
    
    <% @reviews.each do |review| %>
      <tr>
        <td> <%= review.movie.name %></td>
        <td> <%= review.user.username %></td>
        <td> <%= review.created_at %></td>
      </tr>
    <% end %>

  </tbody>
</table>
